fn and (a, b) {
	if (a) {
		if (b) {
			return 1;
		};
	};
	return 0;
}

fn or (a, b) {
	if (a) {
		return 1;
	};
	if (b) {
		return 1;
	};
	return 0;
}

fn rec_fact (n) {
	if (n < 1) {
		return 1;
	} else {
		return n * rec_fact(n - 1);
	};
}

fn fact (n) {
	var acc = 1;
	var i = 1;
	while (i <= n) {
		acc = acc * i;
		i = i + 1;
	};
	return acc;
}

fn main () {
	return fact(5);
}